{
	"$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
	"spaces": 2,
	"generator-cli": {
		"version": "7.14.0",
		"generators": {
			"typescript-fetch": {
				"generatorName": "typescript-fetch",
				"output": "./src/lib/api",
				"glob": "**/*",
				"inputSpec": "http://localhost:5006/swagger/v1/swagger.json",
				"skipValidateSpec": true,
				"additionalProperties": {
					"typescriptThreePlus": true,
					"supportsES6": true,
					"npmName": "perry-chick-api",
					"npmVersion": "1.0.0",
					"stringEnums": true,
					"enumPropertyNaming": "camelCase"
				}
			}
		}
	}
}
